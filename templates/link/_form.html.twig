{{ form_start(form) }}

{{ form_row(form.name) }}

<div class="d-flex justify-content-between align-items-end">
	<div class="form-group w-25 mr-4">
		{{ form_label(form.link) }}
		{{ form_widget(form.link) }}
		{{ form_help(form.link) }}
		{{ form_errors(form.link) }}
	</div>
	<div class="alert alert-info" role="alert">
		Votre lien doit commencer avec http:// ou https:// et doit finir avec un pointquelquechose.
	</div>
</div>

{{ form_rest(form) }}
<button class="btn btn-primary">{{ button_label|default('Ajouter') }}</button>
{{ form_end(form) }}
